include ./mixins.pug
doctype html
html(lang="ru")
	+head('Catalog-2')
	body.page
		.page__body
			+header()
			main.main.page__main
				.page__wrapper
					.breadcrumbs
						a.breadcrumbs__item(href="#")
							| Каталог
						a.breadcrumbs__item(href="#")
							| Коллекционный стада
					h2.page__title
						| Коллекционный стада
					.blog
						.blog__content
							.catalog
								.catalog__items
									a.catalog__item(href="#")
										span.item__pic
											img.img(src="../images/catalog-2-1.jpg", alt="catalog item" data-bg="true")
										span.item__title
											| Куры
									a.catalog__item(href="#")
										span.item__pic
											img.img(src="../images/catalog-2-2.jpg", alt="catalog item" data-bg="true")
										span.item__title
											| Цесарки
									a.catalog__item(href="#")
										span.item__pic
											img.img(src="../images/catalog-2-3.jpg", alt="catalog item" data-bg="true")
										span.item__title
											| Перепела
						aside.blog__aside
			include partials/footer.pug